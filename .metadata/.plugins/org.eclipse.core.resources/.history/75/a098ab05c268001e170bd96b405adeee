import java.io.*;
import java.util.*;


public class Main {
	
	int fact(int n) {
		if (n==1) return 1; else return n*=fact(n-1);
	}
	
	void solve(Scanner in, PrintWriter out) {
		int a = in.nextInt();
		int s = fact(a);
		out.println(s);
	}
	
	void run() {
		try (
			Scanner in = new Scanner(System.in);
			PrintWriter out = new PrintWriter(System.out)
		){
			solve(in, out);			
		}
	}
	
	public static void main(String args[]) {
		new Main().run();
	}
	

}
